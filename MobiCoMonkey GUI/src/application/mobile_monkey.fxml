<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FxController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="config" layoutX="201.0" layoutY="39.0" />
            <Text layoutX="24.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Configuration File" />
            <TextField fx:id="min_inteval" disable="true" layoutX="202.0" layoutY="78.0" />
            <Text layoutX="25.0" layoutY="97.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Minimum Interval" />
            <TextField fx:id="max_interval" disable="true" layoutX="203.0" layoutY="117.0" />
            <Text layoutX="26.0" layoutY="136.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Maximum Interval" />
            <TextField fx:id="duration" disable="true" layoutX="203.0" layoutY="157.0" />
            <Text layoutX="26.0" layoutY="176.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Duration" />
            <TextField fx:id="seed" disable="true" layoutX="204.0" layoutY="197.0" />
            <Text layoutX="27.0" layoutY="216.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Seed" />
            <Button fx:id="run" layoutX="30.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleRunButtonAction" text="Run Test" />
            <Button fx:id="stop" layoutX="130.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleStopButtonAction" text="Stop" />
            <Button fx:id="result" layoutX="203.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleResultButtonAction" text="Result" />
            <Button fx:id="browse" layoutX="394.0" layoutY="39.0" mnemonicParsing="false" onAction="#handleBrowseButtonAction" text="Browse" />
            <Button fx:id="gsm" layoutX="27.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleGsmButtonAction" text="GSM Profile" />
            <Button fx:id="network_status" layoutX="142.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleNsButtonAction" text="Network Status" />
            <Button fx:id="network_delay" layoutX="280.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleNdButtonAction" text="Network Delay" />
            <Text layoutX="26.0" layoutY="325.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Event Generation" />
      </children>
    </AnchorPane>
  </children>
</VBox>
